{
  "compilerOptions": {
      "esModuleInterop": true,
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "esnext",
      "allowJs": true,
      "moduleResolution": "node",
      "rootDir": "src/"
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules",
      "dist",
      "release"
  ]
}
